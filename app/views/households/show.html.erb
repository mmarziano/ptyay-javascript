<div class="clear"></div>

<div class="frame text-center">
    <h1>Household Information</h1>
</div>

<div class="frame text-center">
    <table>
        <tr>
            <th><h3>Parent(s)/Guardian(s)</h3></th>
        </tr>
        <tr>
            <% @household.users.each do |u| %>
                <td><%= u.full_name %></td>
            <% end %>
        </tr>
    </table>
</div>

<div class="clear"></div>

<div class="frame text-center">
    <table>
        <tr>
            <th><h3>Student(s)</h3></th>
            <hr>
        </tr>
        <tr>
            <% @household.students.each do |s| %>
                <td><%= s.full_name %></td>
            <% end %>
        </tr>
    </table>
   
</div>